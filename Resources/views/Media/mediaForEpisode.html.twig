<div class="list-group">
    {% for media in medias %}
        <a href="{{ path('oktolab_media_show_media', {'media': media.id })}}" class="list-group-item">{{ media.quality }}</a>
    {% endfor %}
    {% if medias is empty %}
        <a href="#">{{ 'oktolab_media_media_fore_episode_empty'|trans }}</a>
    {% endif %}
</div>
